#!/usr/bin/perl

$XMLFILE=@ARGV[0];


while (<>) {
  $str=$_;
  while ( $str=~ /<Firewall / ) {
    $str=~ /<Firewall [^>]+name="([^"]*).*$"/;
    $fw=$1;
    printf "echo ====================== $fw =========================================\n";
    printf "fwb_iosacl -xt -v  -f $XMLFILE  $fw\n";
    $str=~ s/^.*<Firewall [^>]+name="$fw"[^>]+>//;
  }
}


