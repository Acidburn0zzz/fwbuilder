<?xml version="1.0"?> <!-- -*- mode: xml; -*- -->
<FWBuilderResources version="3.0.6-">
    <Target name="pix_os">
      <description>Cisco PIX</description>
      <compiler>fwb_pix</compiler>
      <family>pix_os</family>
      <dialog>pix_os</dialog>

      <options>
        <user_can_change_install_dir>false</user_can_change_install_dir>
        <default>
        </default>

        <activation>
          <fwdir></fwdir>
          <fwdir_test></fwdir_test>

          <timeout_units>min</timeout_units>

          <reg_user>
            <run>
              <pre_config_commands>
              </pre_config_commands>

              <post_config_commands>
                wr mem
              </post_config_commands>

              <schedule_rollback>
                reload in %RBTIMEOUT%
              </schedule_rollback>

              <cancel_rollback>
                reload cancel
              </cancel_rollback>

              <save_standby>
                wr standby
              </save_standby>
            </run>

            <test>
              <pre_config_commands>
              </pre_config_commands>

              <post_config_commands>
              </post_config_commands>

              <schedule_rollback>
                reload in %RBTIMEOUT%
              </schedule_rollback>

              <cancel_rollback>
                reload cancel
              </cancel_rollback>

              <save_standby>
                wr standby
              </save_standby>

            </test>

          </reg_user>

        </activation>


      </options>

      <capabilities>
        <supports_routing>False</supports_routing>
      </capabilities>

    </Target>

</FWBuilderResources>
